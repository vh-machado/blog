<h1>Login page</h1>
<%= form_with model: @user, url: login_path do |f| %>
  <div>
    <%= f.label :login %><br>
    <%= f.text_field :login %>
  </div>
  <div>
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </div>
  <p>
    <%= f.submit 'Login' %>
  </p>
<% end %>